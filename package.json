{
  "name": "json-decoder",
  "version": "1.0.0",
  "description": "Helps you transform JSON values into Typescript values",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest --config jestconfig.json",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cjduncana/json-decoder.git"
  },
  "keywords": [
    "decoder",
    "typescript",
    "json"
  ],
  "author": "Christopher Duncan <cj.duncan.a@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cjduncana/json-decoder/issues"
  },
  "homepage": "https://github.com/cjduncana/json-decoder#readme",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "fp-ts": "^1.8.1"
  }
}
